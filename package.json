{
  "name": "typeorm-bug-report-join-table-composite-primary-key",
  "version": "1.0.0",
  "description": "TypeORM bug report for calculating a WHERE clause for a find operation on a join table with custom properties where one of the join partners has a composite primary key.",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "files": [
    "src/*",
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p ./",
    "debug-ts": "TS_NODE_PROJECT=tsconfig.json TS_NODE_FILE=true node -r ts-node/register --inspect=0.0.0.0 --inspect-brk src/app.ts",
    "debug-node": "node --inspect=0.0.0.0 --inspect-brk dist/app.js",
    "start": "ts-node --project tsconfig.json src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derbenoo/typeorm-bug-report-join-table-composite-primary-key.git"
  },
  "keywords": [],
  "author": "Benjamin Assadsolimani",
  "license": "MIT",
  "homepage": "https://derbenoo.github.io/typeorm-bug-report-join-table-composite-primary-key/",
  "devDependencies": {
    "@types/node": "^12.6.9",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18"
  }
}
